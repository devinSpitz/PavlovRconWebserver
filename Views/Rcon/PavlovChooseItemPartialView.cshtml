<!-- Modal -->
<div class="modal fade" id="add-contact" tabindex="-1" role="dialog" aria-labelledby="addContactLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content cssModalSizeAsChildren">
            <div class="modal-header">
                <h5 class="modal-title" id="addContactLabel">Choose item</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </button>
            </div>
            <div class="modal-body">
                @foreach (var list in items)
                {
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            @list.Key
                        </div>
                        <div class="panel-body">
                            @foreach (var item in list.Value)
                            {
                                <button type="button" class="btn btn-success col-sm-3" data-dismiss="modal" onclick="setItem('@item')">@item</button>
                                <div class="col-sm-1"></div>
                            }
                        </div>
                    </div>
                    

                }

                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>