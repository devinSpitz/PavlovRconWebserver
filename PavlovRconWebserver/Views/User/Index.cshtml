@model IEnumerable<LiteDB.Identity.Models.LiteDbUser>
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<h2>
    Users
    <a type="button" class="btn btn-sm btn-primary" asp-controller="Account" asp-action="Register">
        <span class="fas fa-plus"></span>
    </a>
</h2>
<table class="table table-striped table-responsive">
    <thead>
    <th>
        Id
    </th>
    <th>
        UserName
    </th>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>
                <a class="btn btn-sm btn-danger" href="@Url.Action("DeleteUser", new {user.Id})" onclick="return confirm('Are you sure you want to Delete?')">
                    <span class="fas fa-trash-alt"></span>
                </a>
                @user.Id
            </td>
            <td>
                @user.UserName
            </td>
        </tr>
    }
    </tbody>
</table>